# Template for a new network (copy into networks/<name>/.env)
NETWORK_NAME=customnet
NETWORK_ID=123456

# RPC / WS ports on host
RPC_PORT=9545
WS_PORT=9546

# Gateway / Explorer / Faucet ports on host
GATEWAY_PORT=9111
EXPLORER_PORT=9400
FAUCET_PORT=9800

# Geth connectivity
BOOTNODES=
STATIC_PEERS=

# Blockscout DB
POSTGRES_USER=blockscout
POSTGRES_PASSWORD=blockscout
POSTGRES_DB=blockscout
SECRET_KEY_BASE=replace_with_secret

# Faucet
FAUCET_PRIVATE_KEY=
FAUCET_RPC_URL=http://node:8545
FAUCET_AMOUNT=1
